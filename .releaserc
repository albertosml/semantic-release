{
  "branches": ["main"],
  "plugins": [
    [
        "@semantic-release/commit-analyzer",
        {
            "preset": "conventionalcommits"
        }
    ],
    [
        "@semantic-release/release-notes-generator",
        {
            "preset": "conventionalcommits",
            "presetConfig": {
              "types": [
                { "type": "breaking", "section": "❗ Breaking ❗" },
                { "type": "feat", "section": "✨ Feature" },
                { "type": "fix", "section": "🐛 Bugfix" },
                { "type": "docs", "section": "📝  Documentation" },
                { "type": "chore", "section": "🔨  Misc." },
                { "type": "ci", "section": "📦️ Build and CI" },
                { "type": "refactor", "section": "⚡️ Code Performance" },
                { "type": "test", "section": "🧪 Code Test" },
                { "type": "style", "section": "💄 Design" }
              ]
            }
        }
    ],
    [
      "@semantic-release/changelog", {
        "changelogTitle": "# Changelog"
      }
    ],
    [
      "@semantic-release/git", {
        "assets": ["CHANGELOG.md"],
        "message": "chore: release ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ],
  "repositoryUrl": "git@github.com:albertosml/semantic-release.git"
}