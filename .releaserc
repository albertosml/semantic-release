{
  "branches": ["main"],
  "plugins": [
    [
        "@semantic-release/commit-analyzer",
        {
            "preset": "conventionalcommits"
        }
    ],
    [
        "@semantic-release/release-notes-generator",
        {
            "preset": "conventionalcommits",
            "presetConfig": {
              "types": [
                {
                  "type": "breaking",
                  "section": "â— Breaking â—",
                  "hidden": false
                },
                { "type": "feat", "section": "âœ¨ Feature", "hidden": false },
                { "type": "fix", "section": "ğŸ› Bugfix", "hidden": false },
                { "type": "docs", "section": "ğŸ“  Documentation", "hidden": false },
                {
                  "type": "chore",
                  "section": "ğŸ”¨  Misc.",
                  "hidden": false
                },
                {
                  "type": "ci",
                  "hidden": false
                },
                {
                  "type": "refactor",
                  "section": "âš¡ï¸ Code Performance",
                  "hidden": false
                },
                {
                  "type": "test",
                  "section": "ğŸ§ª Code Test",
                  "hidden": false
                },
                {
                  "type": "style",
                  "section": "ğŸ’„ Design",
                  "hidden": false
                },
                { "type": "hotfix", "section": "ğŸ”¥ Hotfix ğŸ”¥", "hidden": false }
              ]
            }
        }
    ],
    [
      "@semantic-release/changelog", {
        "changelogTitle": "# Changelog"
      }
    ],
    [
      "@semantic-release/git", {
        "assets": ["CHANGELOG.md"],
        "message": "chore: release ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ],
  "repositoryUrl": "git@github.com:albertosml/semantic-release.git"
}