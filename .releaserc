{
  "branches": ["main"],
  "plugins": [
    [
        "@semantic-release/commit-analyzer",
        {
            "preset": "conventionalcommits"
        }
    ],
    [
        "@semantic-release/release-notes-generator",
        {
            "preset": "conventionalcommits",
            "presetConfig": {
              "types": [
                { "type": "breaking", "section": "â— Breaking â—" },
                { "type": "feat", "section": "âœ¨ Feature" },
                { "type": "fix", "section": "ğŸ› Bugfix" },
                { "type": "docs", "section": "ğŸ“  Documentation" },
                { "type": "chore", "section": "ğŸ”¨  Misc." },
                { "type": "ci", "section": "ğŸ“¦ï¸ Build and CI" },
                { "type": "refactor", "section": "âš¡ï¸ Code Performance" },
                { "type": "test", "section": "ğŸ§ª Code Test" },
                { "type": "style", "section": "ğŸ’„ Design" }
              ]
            }
        }
    ],
    [
      "@semantic-release/changelog", {
        "changelogTitle": "# Changelog"
      }
    ],
    [
      "@semantic-release/git", {
        "assets": ["CHANGELOG.md"],
        "message": "chore: release ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ],
  "repositoryUrl": "git@github.com:albertosml/semantic-release.git"
}